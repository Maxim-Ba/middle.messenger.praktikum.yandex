{
  "name": "project-chat",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "scripts": {
    "watch": "parcel serve \"src/pages/**/*.html\" --open --no-cache",
    "build:build2": "parcel build \"src/pages/index.html\" --public-url ./ --no-cache --no-content-hash",
    "build:parcel": "npm run clean && npm run build:build2",
    "clean": "rimraf dist/*",
    "server": "node server.js",
    "start": "npm run build && npm run server",
    "lint": "eslint . --fix",
    "stylelint": "npx stylelint \"**/*.scss\" --fix",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha",
    "serve": "webpack serve --mode=development --progress",
    "build": "npm run clean && webpack --config webpack.config.prod.js",
    "precommit": "npm-run-all lint --parallel stylelint test",
    "prepare": "husky install"
  },
  "husky":{
    "hooks":{
      "pre-commit":"npm run precommit"
    }
  },
