{
  "name": "project-chat",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "scripts": {
    "watch": "parcel serve \"src/pages/**/*.html\" --open --no-cache",
    "build:build": "parcel build \"src/pages/index.html\" --public-url ./ --no-cache --no-content-hash",

    "build:build1": "npm run clean && parcel build \"src/**/*.html\" --public-url ./ --no-content-hash",

    "build:build2": "parcel build \"src/pages/profile/*.html\" \"src/pages/registration/*.html\" \"src/pages/chats/*.html\" \"src/pages/error-page/*.html\" --dist-dir dist/**  --public-url ../ --no-cache --no-content-hash",
    "build": "npm run clean && npm run build:build1",
    "clean": "rm -rf dist/*",
    "server": "node server.js",
    "start": "npm run server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/resolver-glob": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.1",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "parcel": "^2.2.1",
    "parcel-transformer-hbs": "^1.0.4",
    "postcss-import": "^14.0.2",
    "postcss-sass": "^0.5.0",
    "precss": "^4.0.0",
    "sass": "^1.49.7"
  },
  "dependencies": {
    "@anikin/parcel-transformer-handlebars-precompile": "^1.0.1",
    "express": "^4.17.2",
    "handlebars": "^4.7.7",
    "parcel-plugin-handlebars": "^0.4.4",
    "postcss": "^8.4.6"
  }
}
