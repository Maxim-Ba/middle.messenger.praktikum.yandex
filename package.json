{
  "name": "project-chat",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "scripts": {
    "watch": "parcel serve \"src/pages/**/*.html\" --open --no-cache",
    "build:build1": "parcel build \"src/pages/profile/*.html\" \"src/pages/login/*.html\" \"src/pages/registration/*.html\" \"src/pages/chats/*.html\" \"src/pages/error-page/*.html\" --dist-dir dist/**  --public-url ../ --no-cache --no-content-hash --no-optimize",
    "build:build2": "parcel build \"src/pages/index.html\"  --public-url ./ --no-cache",
    "build": "npm run clean && npm run build:build2",
    "clean": "rimraf dist/*",
    "server": "node server.js",
    "start": "npm run build && npm run server",
    "lint": "eslint . --fix",
    "stylelint": "npx stylelint \"**/*.scss\" --fix",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.7",
    "@parcel/resolver-glob": "2.2.1",
    "@parcel/transformer-postcss": "2.3.2",
    "@parcel/transformer-sass": "2.3.2",
    "@types/chai": "4.3.0",
    "@types/jsdom": "16.2.14",
    "@types/mocha": "9.1.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "autoprefixer": "10.4.2",
    "chai": "4.3.6",
    "cross-env": "^7.0.3",
    "cssnano": "5.0.16",
    "eslint": "8.9.0",
    "jsdom": "19.0.0",
    "mocha": "9.2.2",
    "parcel": "2.3.2",
    "parcel-transformer-hbs": "1.0.4",
    "postcss-import": "14.0.2",
    "postcss-lit": "0.4.3",
    "postcss-sass": "0.5.0",
    "precss": "4.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sass": "1.49.7",
    "stylelint": "14.5.3",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "express": "4.17.2",
    "handlebars": "4.7.7",
    "parcel-plugin-handlebars": "0.4.4",
    "postcss": "8.4.6",
    "uuid": "8.3.2"
  }
}
